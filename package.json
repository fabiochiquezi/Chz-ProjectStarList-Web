{
  "name": "project-open-front-mystarlist",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "emulators": "firebase emulators:start --import=./emulators-data",
    "emulators:data": "firebase emulators:start --import=./new-emulators-data --export-on-exit",
    "development": "next dev",
    "dev": "concurrently --kill-others \"yarn emulators\" \"yarn development\"",
    "build": "rm -rf out && next build",
    "start": "next start",
    "e2e": "concurrently --kill-others \"yarn development\" \"yarn cypress\"",
    "cypress": "cypress open",
    "test": "jest --runInBand --watch",
    "test:coverage": "jest --coverage --runInBand --watch"
  },
